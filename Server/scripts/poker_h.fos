    DlgTxt[ pokerMan.StatBase[ ST_VAR9 ] ] = "@msg dlg " + DLGSTR( DID, STR_CARD_IN_SLEEVE ) + "@ " + GetStrCard( CardInSleeve[ pokerMan.StatBase[ ST_VAR9 ] ] ) + "\n" + DlgTxt[ pokerMan.StatBase[ ST_VAR9 ] ];
