        if( not ( GetRank( GameCards[ pokerMan.StatBase[ ST_VAR9 ] ][ i * CARD_COUNT + c - 1 ] ) == hc ) )
        {
            ReplCards[ pokerMan.StatBase[ ST_VAR9 ] ][ i ] = SetBit5( ReplCards[ pokerMan.StatBase[ ST_VAR9 ] ][ i ], c, true );
        }
    }
}

