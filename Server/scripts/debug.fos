void Unblock( Critter& player, int radius, int, int )
{
    Map@   m = player.GetMap();
    uint16 x = player.HexX;
    uint16 y = player.HexY;
    for( int i = 1; i < radius; i++ )
    {
        m.UnblockHex( x + i, y );
        m.UnblockHex( x - i, y );
        m.UnblockHex( x + i, y - i );
        m.UnblockHex( x - i, y - i );
        m.UnblockHex( x + i, y + i );
        m.UnblockHex( x - i, y + i );
        m.UnblockHex( x, y - i );
        m.UnblockHex( x, y + i );
    }
}

