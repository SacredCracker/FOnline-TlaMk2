void ToContainer( Critter& cr, int id, int, int )
{
    Item@ cont = GetItem( id );
    if( @cont == null )
    {
        cr.Say( SAY_NETMSG, "Container " + id + " not found!" );
        return;
    }
    Map@ map = cr.GetMap();
