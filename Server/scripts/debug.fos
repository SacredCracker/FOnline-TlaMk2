class DoubleUint16
{
    DoubleUint16( uint16 a = 0, uint16 b = 0 )
    {
        A = a;
        B = b;
    }

    bool IsDouble( uint16 a = 0, uint16 b = 0 )
    {
        return ( A == a && B == b );
    }

    bool IsDouble( DoubleUint16& other )
    {
        return ( A == other.A && B == other.B );
    }

    uint16 A;
    uint16 B;
}

