        max = MAX( min, max );
        best = CLAMP( best, min, max );

        npc.Say( SAY_NETMSG, "min " + min + " best " + best + " max " + max + " wpnDist " + wpnDist + " targetWpnDist " + targetWpnDist );

        /*r1 = min;

